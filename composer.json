{
  "name": "apie/typescript-client-builder",
  "description": "Composer package of the apie library: typescript client builder",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\TypescriptClientBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\TypescriptClientBuilder\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "apie/common": "self.version",
    "apie/core": "self.version",
    "fakerphp/faker": "^1.23.1",
    "nyholm/psr7": "^1.8.2",
    "psr/http-message": "^1.1"
  },
  "require-dev": {
        "apie/fixtures": "self.version",
    "illuminate/support": "*",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}